APP_ENV=dev
SERVER_ADDR=localhost:8080
LOG_LEVEL=info
LOG_FORMAT=text

# PostgreSQL
POSTGRES_URL=postgres://postgres:postgres@localhost:5432/app?sslmode=disable
PG_MAX_OPEN=10
PG_MAX_IDLE=5

# Redis
REDIS_ADDR=localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# RabbitMQ
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# Elasticsearch
# 多个地址用逗号分隔，如 http://localhost:9200,http://es2:9200
ES_ADDRS=http://localhost:9200
ES_USERNAME=
ES_PASSWORD=

# Apollo
APOLLO_ENABLE=false
APOLLO_APP_ID=your-app-id
APOLLO_CLUSTER=default
APOLLO_NAMESPACE=application
# 多个服务地址用逗号分隔，如：http://apollo-1:8080,http://apollo-2:8080
APOLLO_ADDRS=http://localhost:8080
APOLLO_ACCESS_KEY=
